@using TravelTripProject.Models.Classes
@model List<Hakkimizda>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkımızda</h3>

            @foreach (var x in Model)
            {
                <img src="@x.FotoUrl" alt="foto" />
                <p style="font-size:large" >@x.Aciklama</p>
            }

        </div>
    </div>
</div>


